<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.juniorkg.juniorgeneralawarness.JuniorGeneralActivity10">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <include layout="@layout/header" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <RelativeLayout
                    android:id="@+id/rl1"
                    android:layout_width="match_parent"
                    android:layout_height="700sp">

                </RelativeLayout>

                <RadioButton
                    android:id="@+id/one"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_marginLeft="@dimen/splash_img"
                    android:layout_marginTop="@dimen/fifty" />

                <RadioButton
                    android:id="@+id/onel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"

                    android:layout_marginEnd="@dimen/splash_img"
                    android:layout_marginTop="@dimen/fifty" />

                <RadioButton
                    android:id="@+id/two"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_marginLeft="@dimen/splash_img"
                    android:layout_marginTop="@dimen/onesixty" />

                <RadioButton
                    android:id="@+id/twol"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"

                    android:layout_marginEnd="@dimen/splash_img"
                    android:layout_marginTop="@dimen/onesixty" />

                <RadioButton
                    android:id="@+id/three"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_marginLeft="@dimen/splash_img"
                    android:layout_marginTop="@dimen/twoseventy" />

                <RadioButton
                    android:id="@+id/threel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="@dimen/splash_img"
                    android:layout_marginTop="@dimen/twoseventy" />

                <RadioButton
                    android:id="@+id/four"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_marginLeft="@dimen/splash_img"
                    android:layout_marginTop="@dimen/threeeighty" />

                <RadioButton
                    android:id="@+id/fourl"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"

                    android:layout_marginEnd="@dimen/splash_img"
                    android:layout_marginTop="@dimen/threeeighty" />

                <RadioButton
                    android:id="@+id/five"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_marginLeft="@dimen/splash_img"
                    android:layout_marginTop="@dimen/fourninty" />

                <RadioButton
                    android:id="@+id/fivel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="@dimen/splash_img"
                    android:layout_marginTop="@dimen/fourninty" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/monkey"
                    android:layout_marginTop="@dimen/twenty"
                    android:layout_marginStart="@dimen/fifteen"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    app:cardCornerRadius="@dimen/twenty">

                    <ImageView
                        android:layout_gravity="center"
                        android:layout_width="@dimen/seventyfive"
                        android:layout_height="@dimen/seventyfive"
                        android:layout_margin="@dimen/ten"
                        android:src="@drawable/pichkarijg10" />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/rabittail"
                    android:layout_marginEnd="@dimen/fifteen"
                    android:layout_marginTop="@dimen/twenty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="@dimen/twenty"
                    android:layout_alignParentEnd="true"
                    android:layout_gravity="center">

                    <ImageView
                        android:layout_width="@dimen/seventyfive"
                        android:layout_height="@dimen/seventyfive"
                        android:src="@drawable/diwalijg10"
                        android:gravity="center"
                        android:layout_margin="@dimen/ten"
                        android:layout_gravity="center" />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/rabit"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="@dimen/fifteen"
                    android:layout_marginTop="@dimen/onethirty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="@dimen/twenty">

                    <ImageView
                        android:layout_gravity="center"
                        android:layout_width="@dimen/seventyfive"
                        android:layout_height="@dimen/seventyfive"
                        android:layout_margin="@dimen/ten"
                        android:src="@drawable/eidjg10" />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/dogtail"
                    android:layout_marginEnd="@dimen/fifteen"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="@dimen/onethirty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="@dimen/twenty"
                    android:layout_gravity="center">

                    <ImageView
                        android:layout_width="@dimen/seventyfive"
                        android:layout_height="@dimen/seventyfive"
                        android:src="@drawable/christmasjg10"
                        android:gravity="center"
                        android:layout_margin="@dimen/ten"
                        android:layout_gravity="center" />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/elephant3"
                    android:layout_marginStart="@dimen/fifteen"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/twoforty"
                    app:cardCornerRadius="@dimen/twenty">

                    <ImageView
                        android:layout_gravity="center"
                        android:layout_width="@dimen/seventyfive"
                        android:layout_height="@dimen/seventyfive"
                        android:layout_margin="@dimen/ten"
                        android:src="@drawable/christmastreejg10" />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/pigtail"
                    android:layout_marginEnd="@dimen/fifteen"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    app:cardCornerRadius="@dimen/twenty"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="@dimen/twoforty"
                    android:layout_gravity="center">

                    <ImageView
                        android:layout_width="@dimen/seventyfive"
                        android:layout_height="@dimen/seventyfive"
                        android:src="@drawable/pongaljg10"
                        android:gravity="center"
                        android:layout_margin="@dimen/ten"
                        android:layout_gravity="center" />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/pig"
                    android:layout_marginStart="@dimen/fifteen"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="@dimen/threefifty"
                    app:cardCornerRadius="@dimen/twenty">

                    <ImageView
                        android:layout_gravity="center"
                        android:layout_width="@dimen/seventyfive"
                        android:layout_height="@dimen/seventyfive"
                        android:layout_margin="@dimen/ten"
                        android:src="@drawable/crackersjg10" />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/elephanttail"
                    android:layout_marginTop="@dimen/threefifty"
                    android:layout_width="wrap_content"
                    android:layout_marginEnd="@dimen/fifteen"
                    android:layout_alignParentEnd="true"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    app:cardCornerRadius="@dimen/twenty">

                    <ImageView
                        android:layout_width="@dimen/seventyfive"
                        android:layout_height="@dimen/seventyfive"
                        android:src="@drawable/holijg10"
                        android:gravity="center"
                        android:layout_margin="@dimen/ten"
                        android:layout_gravity="center" />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/dog"
                    android:layout_marginStart="@dimen/fifteen"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="@dimen/foursixty"
                    app:cardCornerRadius="@dimen/twenty">

                    <ImageView
                        android:layout_gravity="center"
                        android:layout_width="@dimen/seventyfive"
                        android:layout_height="@dimen/seventyfive"
                        android:layout_margin="@dimen/ten"
                        android:src="@drawable/sugarcanesjg10" />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/monkeytail"
                    android:layout_marginEnd="@dimen/fifteen"
                    android:layout_marginTop="@dimen/foursixty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    app:cardCornerRadius="@dimen/twenty">

                    <ImageView
                        android:layout_width="@dimen/seventyfive"
                        android:layout_height="@dimen/seventyfive"
                        android:src="@drawable/ramzanjg10"
                        android:gravity="center"
                        android:layout_margin="@dimen/ten"
                        android:layout_gravity="center" />

                </androidx.cardview.widget.CardView>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/fiveseventy"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:orientation="vertical">

                            <androidx.cardview.widget.CardView
                                android:id="@+id/back"
                                android:layout_width="@dimen/onethirty"
                                android:layout_margin="@dimen/twenty"
                                android:layout_gravity="left"
                                android:layout_height="@dimen/fifty"
                                app:cardBackgroundColor="@color/colorAccent"
                                app:cardCornerRadius="@dimen/twenty">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:src="@drawable/backwhite"
                                        android:layout_marginLeft="@dimen/ten"
                                        android:layout_marginStart="@dimen/ten"
                                        android:layout_gravity="center_vertical"/>

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:gravity="center"
                                        android:layout_height="match_parent"
                                        android:text="@string/back"
                                        android:textColor="@android:color/white"
                                        android:layout_marginLeft="@dimen/ten"
                                        android:layout_marginStart="@dimen/ten"
                                        android:layout_gravity="center"
                                        android:textStyle="bold"
                                        android:textSize="@dimen/twenty"/>

                                </LinearLayout>

                            </androidx.cardview.widget.CardView>

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </RelativeLayout>

        </LinearLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>